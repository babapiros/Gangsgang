<main class="text-light">
   	<style>
		body {
			background: url(https://cdn.glitch.global/16ff2add-476f-4417-b371-338021fa6c55/1558d4ff-5947-458c-a811-2309388bf26b.image.png?v=1670079594434) no-repeat fixed;
			background-size: 100%;
		}
	</style>
<html>
  <body>
    {{> partials/_header}}
    {{> partials/_loader}}  

    <div class="team" id="staffs">
      <h1>Yetkililerimiz</h1>
      <br>
      <div class="team-cards">
        {{#each owners}}
          <div class="{{this.user.username}}" onclick="window.open('https://discordapp.com/users/{{this.user.id}}')">
            <h5 style="font-family: sans-serif; font-weight: normal">
              {{this.user.username}}
            </h5>
            <h6 style="color: #f8a5a5">Owner</h6>
            <img src="{{this.user.avatarURL dynamic=true size=2048}}" />
            <span class="{{this.user.presence.status}}"></span>
          </div>
        {{/each}}
        {{#each admins}}
          <div class="{{this.user.username}}" onclick="window.open('https://discordapp.com/users/{{this.user.id}}')">
            <h5 style="font-family: sans-serif; font-weight: normal">
              {{this.user.username}}
            </h5>
            <h6 style="color: #caaafd">Admin</h6>
            <img src="{{this.user.avatarURL dynamic=true size=2048}}" />
            <span class="{{this.user.presence.status}}"></span>
          </div>
        {{/each}}
        {{#each codeSharers}}
          <div class="{{this.user.username}}" onclick="window.open('https://discordapp.com/users/{{this.user.id}}')">
            <h5 style="font-family: sans-serif; font-weight: normal">
              {{this.user.username}}
            </h5>
            <h6 style="color: #f3b7ff">Code Sharer</h6>
            <img src="{{this.user.avatarURL dynamic=true size=2048}}" />
            <span class="{{this.user.presence.status}}"></span>
          </div>
        {{/each}}
      </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.5.0.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/index.js"></script>
    {{> partials/_footer}}
  </body>
</html>
