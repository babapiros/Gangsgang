<title>{{member.user.username}}</title>

  <div class="twPc-div bg-dark">

    <a class="twPc-bg twPc-block"></a>

    <div>

      <div class="twPc-button">

        <a href="https://discordapp.com/users/{{member.user.id}}" class="btn btn-primary btn-sm">Discord Link</a>  

      </div>

      <a href="https://discordapp.com/users/{{member.user.id}}" class="twPc-avatarLink">

        <img alt="Avatar" src="{{avatarURL}}" class="twPc-avatarImg">

      </a>

      <div class="twPc-divUser">

        <div class="twPc-divName">

          {{member.user.username}}

          

        </div>

        <span> 

         {{#if perms.owner}}

                        <span><img src="https://i.ibb.co/Thqb9KJ/R-removebg-preview.png" alt="Owner" width="50" height="30">Owner</span>

                    {{else}}

                        {{#if perms.admin}}

                            <span><img src="https://i.ibb.co/4tQbz1N/9111-shinyblueearlydev-1-removebg-preview.png" alt="Administrator" width="30" height="30">Administrator</span>

                        {{else}}

                            {{#if perms.yetkili}}

                                <span><img src="https://i.ibb.co/bLzVZVX/OIP-5-removebg-preview.png" alt="Code Sharing Administrator" width="30" height="30">Code Sharing Administrator</span>

                            {{else}}

                                <span><img src="https://i.ibb.co/PzhvDnt/pet.gif" alt="Member" width="30" height="30">Member</span>

                            {{/if}}

                        {{/if}}

                    {{/if}}

                </span>

                {{#if stats}}

                    <span> |  Paylaşılan {{stats.paylasilan}} - Silinen {{stats.silinen}} </span>

                {{/if}}

            </div>

            <div class="twPc-divStats">

                <h3>The codes they can access</h3>

                <ul class="twPc-Arrange">

                    <li class="twPc-ArrangeSizeFit"><span class="twPc-StatValue"><a class="btn btn-outline-warning" href="/script">Roblox Script</a></span></li>

                    <li class="twPc-ArrangeSizeFit"><span class="twPc-StatValue"><a class="btn btn-outline-warning" href="/topluluk">Public</a></span></li>

                    {{#if perms.owner}}

                        <li class="twPc-ArrangeSizeFit"><span class="twPc-StatValue"><a class="btn btn-outline-warning" href="/">Exclusive codes</a></span></li>

                    {{else}}

                        {{#if perms.admin}}

                            <li class="twPc-ArrangeSizeFit"><span class="twPc-StatValue"><a class="btn btn-outline-warning" href="/">Exclusive codes</a></span></li>

                        {{else}}

                            {{#if perms.yetkili}}{{/if}}

                                <li class="twPc-ArrangeSizeFit"><span class="twPc-StatValue"><a class="btn btn-outline-warning" href="/">Exclusive codes</a></span></li>

                            {{#if perms.elmas}}{{/if}}

                            {{#if perms.hazir}}{{/if}}

                        {{/if}}

                    {{/if}}

                </ul>

	<!-- KAR--> <script type="text/javascript" src="https://webkodu.ozgurlukicin.com/kod-kaynak/wk-kar-efekt.js"></script> <!-- KAR-->

       </div>

     </div>

   </div>

 </div>
